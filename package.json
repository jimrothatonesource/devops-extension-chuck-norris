{
  "name": "chucknorris-build-enhancer",
  "version": "1.0.15",
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc -p .",
    "postbuild": "npm run package",
    "package": "node node_modules/tfx-cli/_build/tfx-cli.js extension create --manifest-globs vss-extension.json",
    "clean": "powershell -Command \"if (Test-Path ./dist) { Remove-Item -Recurse -Force ./dist }; Remove-Item -Force *.vsix -ErrorAction SilentlyContinue\"",
    "publish": "node node_modules/tfx-cli/_build/tfx-cli.js extension publish --manifest-globs vss-extension.json --share-with onesourcevirtual"
  },
  "author": "OneSource Virtual",
  "devDependencies": {
    "@types/jquery": "^3.5.31",
    "ncp": "^2.0.0",
    "rimraf": "^6.0.1",
    "tfx-cli": "^0.17.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "azure-devops-extension-api": "^4.251.0",
    "azure-devops-extension-sdk": "^4.0.2",
    "jquery": "^3.7.1"
  }
}
