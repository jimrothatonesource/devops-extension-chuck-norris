{
  "manifestVersion": 1,
  "id": "chucknorris-build-enhancer",
  "version": "1.0.15",
  "name": "Chuck Norris Build Enhancer",
  "scopes": [
    "vso.build"
  ],
  "description": "Add this extension to your build definition and get a Chuck Norris quote to help you keep your builds in shape!",
  "publisher": "jimrothatonesource",
  "public": false,
  "icons": {
    "default": "static/images/logo.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "tags": [
    "Build",
    "Fun"
  ],
  "links": {
    "home": {
      "uri": "https://github.com/jimrothatonesource/devops-extension-chuck-norris"
    },
    "getstarted": {
      "uri": "https://github.com/jimrothatonesource/devops-extension-chuck-norris"
    },
    "learn": {
      "uri": "https://github.com/jimrothatonesource/devops-extension-chuck-norris"
    },
    "support": {
      "uri": "https://github.com/jimrothatonesource/devops-extension-chuck-norris/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/jimrothatonesource/devops-extension-chuck-norris"
  },
  "branding": {
    "color": "rgb(0, 0, 0)",
    "theme": "dark"
  },
  "content": {
    "license": {
      "path": "docs/license.md"
    },
    "details": {
      "path": "docs/overview.md"
    }
  },
  "files": [
    {
      "path": "dist",
      "addressable": true,
      "packagePath": "enhancer"
    },
    {
      "path": "ChuckNorrisTask",
      "addressable": true
    },
    {
      "path": "docs",
      "addressable": true,
      "packagePath": "docs"
    },
    {
      "path": "static",
      "addressable": true,
      "packagePath": "/"
    },
    {
      "path": "node_modules/azure-devops-extension-sdk",
      "addressable": true,
      "packagePath": "lib/SDK"
    },
    {
      "path": "node_modules/azure-devops-extension-api",
      "addressable": true,
      "packagePath": "lib/API"
    },
    {
      "path": "node_modules/jquery/dist",
      "addressable": true,
      "packagePath": "lib/jquery"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "contributions": [
    {
      "id": "chuck-norris-task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "type": "ms.vss-distributed-task.task",
      "properties": {
        "name": "ChuckNorrisTask"
      }
    },
    {
      "id": "chuck-norris-build-status-section",
      "type": "ms.vss-build-web.build-results-section",
      "description": "Adds a section to the build Summary with Chuck Norris's comment about the build.",
      "targets": [
        ".build-info-tab",
        "ms.vss-build-web.build-results-summary-tab"
      ],
      "properties": {
        "name": "Chuck Norris Says",
        "uri": "StatusSection.html",
        "order": 20,
        "height": 150,
        "supportsTasks": [
          "6785970c-2d58-4260-b047-0a54028ee9c1"
        ]
      }
    }
  ]
}
